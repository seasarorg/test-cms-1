    class="org.seasar.framework.aop.interceptors.InterceptorChain">
    <initMethod name="add">
      <arg>j2ee.requiredTx</arg>
    </initMethod>
    <initMethod name="add">
      <arg>asBeanList</arg>
    </initMethod>
  </component>
